    "import numpy as np\n",
    "\n",
    "x = np.array([[1.0,0.8,0.1],\n",
    "              [1.0,0.7,0.9],\n",
    "              [0.9,0.5,0.2],\n",
    "              [0.6,0.6,0.8]])\n",
    "y = np.argmax(x,axis=1)\n",
    "print(\"同一行矩陣最大值位置\",y)\n",
    "y = np.argmax(x,axis=0)\n",
    "print(\"同一列矩陣最大值位置\",y)\n",
    "print(\"矩陣同位置的數值比較\",x[0]==x[1])"
   ]
