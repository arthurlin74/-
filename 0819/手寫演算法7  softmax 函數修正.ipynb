   "source": [
    "import numpy as np\n",
    "\n",
    "def softmax(a):\n",
    "    c =np.max(a) #取最大值\n",
    "    exp_a = np.exp(a - c) #將最大值扣除，使數字為負，減少位數極限問題\n",
    "    sum_exp_a = np.sum(exp_a)\n",
    "    y = exp_a / sum_exp_a\n",
    "    return y\n",
    "\n",
    "#測試\n",
    "\n",
    "a = np.array([1010,1000,990])\n",
    "print(softmax(a))\n"
   ]
  }
